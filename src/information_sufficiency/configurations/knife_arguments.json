{
    "device": "cuda",
    "batch_size": 128,
    "n_epochs": 100,
    "n_epochs_marg": 100,
    "stopping_criterion": "max_epochs",
    "cond_modes": 8,
    "marg_modes": 8,
    "ff_layers": 2,
    "eval_batch_size": 1024,
    "lr": 0.001,
    "margin_lr": 0.0001,
    "ff_layer_norm": true,
    "ff_activation": "relu",
    "optimize_mu": true,
    "cov_diagonal": "var",
    "cov_off_diagonal": "",
    "average": "var",
    "ff_dim_hidden": 0,
    "use_tanh": true,
    "init_std": 0.01,
    "eps": 1e-7,
    "ff_residual_connection": false,
    "num_workers": 2,
    "pin_memory": true,
    "persistent_workers": true,
    "prefetch_factor": 2,
    "drop_last": false
}